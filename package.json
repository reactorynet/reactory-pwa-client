  "scripts": {
    "analyze": "source-map-explorer 'build/reactory/static/js/*.js'",
    "start": "REACT_APP_TITLE=Reactory REACT_APP_THEME=reactory REACT_APP_CLIENT_KEY=reactory node --max_old_space_size=2000000 scripts/start.js",
    "start:towerstone": "REACT_APP_TITLE=\"TowerStone Leadership Centre\" REACT_APP_THEME=towerstone REACT_APP_CLIENT_KEY=towerstone node --max_old_space_size=2000000 scripts/start.js",
