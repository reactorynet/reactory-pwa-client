    "ci:build:qa:reactory": "env-cmd -f config/env/.env.reactory.qa node --max_semi_space_size=2 --max_old_space_size=4096 scripts/build.js",
    "ci:build:prod:reactory": "env-cmd -f config/env/.env.reactory.prod node --max_semi_space_size=2 --max_old_space_size=4096 scripts/build.js",
    "ci:build:qa:towerstone": "env-cmd -f config/env/.env.towerstone.qa node --max_semi_space_size=2 --max_old_space_size=4096 scripts/build.js",
