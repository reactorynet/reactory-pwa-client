{
  "compilerOptions": {    
    "noImplicitAny": false,
    "removeComments": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "target": "es6",
    "jsx": "preserve",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strict": false,
    "lib": [
      "es6",
      "dom"
    ],
    "types": [
      "webpack-env",
      "markerclustererplus"
    ],
    "baseUrl": ".",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "watch": true,
    "paths": {
      "*": [
        "src/*"
      ],
      "@reactory/client-core/*": [
        "src/*"
      ],
      "test/*": [
        "test/*"
      ],
      "@reactory/webpack/*": [
        "webpack/*"
      ],
      "@reactory/config/*": ["config/*"],
      "@rjsf/material-ui/*": [
        "node_modules/@rjsf/material-ui/dist/*"
      ]
    },
    "pretty": true,
    "typeRoots": [
      "./node_modules/@types",
      "src/types"
    ],
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "config",
    "scripts"
  ],
  "include": [
    "src/**/*",
    "src/types/**/*.d.ts"]
  }