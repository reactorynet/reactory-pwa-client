{
  "presets": [
    ["@babel/preset-env"],
    ["@babel/preset-react", {
      "runtime": "automatic"
    }],
    ["@babel/preset-typescript", {
      "isTSX": true,
      "allowNamespaces": true,
      "allExtensions": true
    }]
  ],
  "plugins": [
    ["module-resolver", {
      "root": "./src",
      "alias": {
        "@reactory/client-core": "./src"
      }
    }],
    "@babel/plugin-transform-react-jsx"
  ]
}